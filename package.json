{
    "name": "courseden-rear",
    "version": "0.0.1",
    "description": "",
    "main": "src",
    "scripts": {
        "test": "find ./src -name '*.spec.ts' | xargs ts-mocha test/*.ts",
        "build:ts": "tsc",
        "start": "ts-node src/index.ts",
        "dev": "nodemon"
    },
    "nodemonConfig": {
        "ext": "*.ts,*.json",
        "ignore": [
            "test/**/*.ts"
        ],
        "delay": "2500",
        "exec": "ts-node src/index.ts"
    },
    "prettier": {
        "arrowParens": "avoid",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "printWidth": 120
    },
    "repository": {
        "type": "git",
        "url": "git@oc.kekvrose.me:work/courseden-rear.git/"
    },
    "author": "Atreya Bain",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "pg": "^8.5.1",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^9.0.0",
        "typeorm": "^0.2.29"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/chai": "^4.2.14",
        "@types/expect": "^24.3.0",
        "@types/express": "^4.17.9",
        "@types/mocha": "^8.0.4",
        "@types/morgan": "^1.9.2",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "dotenv": "^8.2.0",
        "mocha": "^8.2.1",
        "nodemon": "^2.0.6",
        "ts-mocha": "^8.0.0",
        "typescript": "^4.0.5"
    }
}
